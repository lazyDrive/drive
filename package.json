{
  "name": "lazyDrive",
  "version": "0.0.1",
  "private": true,
  "author": "Anurag Kumar (anu1601cs)",
  "description": "A file sharing community.",
  "scripts": {
    "lint": "vue-cli-service lint",
    "dev": "npm run lb-start & npm run vue-serve",
    "lb-start": "nodemon",
    "posttest": "npm run lint && nsp check",
    "prod": "environment=production npm run vue-build && npm start",
    "start": "node .",
    "vue-build": "vue-cli-service build",
    "vue-serve": "vue-cli-service serve"
  },
  "main": "server/server.js",
  "dependencies": {
    "axios": "^0.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dropbox": "^4.0.27",
    "express-history-api-fallback": "^2.2.1",
    "file-saver": "^2.0.2",
    "helmet": "^3.18.0",
    "isomorphic-fetch": "^2.2.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "loopback": "^3.26.0",
    "loopback-boot": "^2.28.0",
    "loopback-component-explorer": "^6.4.0",
    "loopback-connector-mongodb": "^4.2.0",
    "register-service-worker": "^1.6.2",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^2.0.0",
    "underscore": "^1.9.1",
    "vue": "^2.6.10",
    "vue-analytics": "^5.17.0",
    "vue-pdf": "^4.0.7",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.1",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.3.4",
    "@vue/cli-plugin-e2e-cypress": "^3.4.1",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-pwa": "^3.4.1",
    "@vue/cli-plugin-unit-jest": "^3.4.1",
    "@vue/cli-service": "^3.4.1",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "cypress": "^3.1.5",
    "eslint": "^5.16.0",
    "eslint-config-loopback": "^8.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-vue": "^5.2.2",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.11.0",
    "nodemon": "^1.19.1",
    "nsp": "^3.2.1",
    "prettier": "^1.18.2",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "engines": {
    "node": ">=4"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "license": "UNLICENSED",
  "lint-staged": {
    "./src/*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "client/*"
    ],
    "delay": "2500"
  },
  "repository": {
    "type": "",
    "url": ""
  }
}
